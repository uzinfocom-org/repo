pkgver=20201117
pkgrel=2
pkgdesc='Xinux PGP keyring'
arch=('any')
url='https://oss.uzinfocom.uz'
license=('GPL')
install="${pkgname}.install"
makedepends=("git")
source=("git+https://github.com/FascodeNet/alterlinux-keyring.git")
